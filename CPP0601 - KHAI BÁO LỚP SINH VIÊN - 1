#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>
#include <iomanip>
using namespace std;
struct SinhVien
{
    string ten, lop, sinh;
    double diem;

    void xuat()
    {
        if (sinh[1] == '/')
            sinh.insert(0, 1, '0');
        if (sinh[4] == '/')
            sinh.insert(3, 1, '0');
        cout << "B20DCCN001 " <<ten << " " << lop << " " << sinh << " " << fixed << setprecision(2) << diem;
    }
    void nhap()
    {
        getline(cin,ten);
        cin >> lop >> sinh >> diem;
    }
};
int main(){
    SinhVien a;
    a.nhap();
    a.xuat();
    return 0;
}
