#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <map>
using namespace std;
bool sosanh(int a, int b)
{
    return a > b;
}
int main()
{
    int m, n;
    cin >> m;
    while (m--)
    {
        cin >> n;
        vector<int> a;
        string s;
        cin >> s;
        for (int i = 0; i < s.size(); i++)
        {
            int tmp = s[i] - '0';
            if (tmp == 2)
                a.push_back(tmp);
            else if (tmp == 3)
                a.push_back(tmp);
            else if (tmp == 4)
            {
                a.push_back(3);
                a.push_back(2);
                a.push_back(2);
            }
            else if (tmp == 5)
                a.push_back(tmp);
            else if (tmp == 6)
            {
                a.push_back(5);
                a.push_back(3);
            }
            else if (tmp == 7)
                a.push_back(tmp);
            else if (tmp == 8)
            {
                a.push_back(7);
                a.push_back(2);
                a.push_back(2);
                a.push_back(2);
            }
            else if (tmp == 9)
            {
                a.push_back(7);
                a.push_back(3);
                a.push_back(3);
                a.push_back(2);
            }
        }
        sort(a.begin(), a.end(), sosanh);
        for (int i = 0; i < a.size(); i++)
        {
            cout << a[i];
        }
        cout << endl;
    }
}
