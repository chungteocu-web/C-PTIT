#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        long long a, b, c, sum = 1;
        cin >> a >> b >> c;
        for (int i = 0; i < b; i++)
        {
            sum *= (a % c);
            sum %= c;
        }
        cout << sum << endl;
    }
}
