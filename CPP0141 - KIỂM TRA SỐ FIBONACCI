#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int uocnt(int m)
{
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        long long a, fn1 = 1, fn2 = 1, fn = fn1 + fn2, check = 1;
        cin >> a;
        if (a < 3)
        {
            cout << "YES" << endl;
            continue;
        }
        while (fn <= a)
        {
            if (fn == a)
            {
                cout << "YES" << endl;
                check = 0;
                break;
            }
            fn1 = fn2;
            fn2 = fn;
            fn = fn1 + fn2;
        }
        if (check == 1)
            cout << "NO" << endl;
    }
}
