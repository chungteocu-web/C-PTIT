#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
using namespace std;
int main()
{
    int m, n, x;
    cin >> m;
    while (m--)
    {
        cin >> n;
        vector<int> a(n), b(n);
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
        }
        int check = 0;
        for (int i = 0; i < n - 1; i++)
        {
            if (a[i + 1] != 0)
            {
                if (a[i] == a[i + 1])
                {
                    a[i] *= 2;
                    a[i + 1] = 0;
                }
            }
        }
        for (int i = 0; i < n; i++)
        {
            if (a[i] != 0)
            {
                b[check++] = a[i];
            }
        }
        for (int i = 0; i < n; i++)
        {
            cout << b[i] << " ";
        }
        cout << endl;
    }
}
