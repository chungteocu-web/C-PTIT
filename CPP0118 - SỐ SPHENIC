#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a, sum = 0,check=0;
        cin >> a;
        for (int i = 2; i <= sqrt(a); i++)
        {
            if (a % i == 0)
            {
                int count = 0;
                while (a % i == 0)
                {
                    ++count;
                    a /= i;
                }
                if(count>1)
                {check=1;}
                if (count == 1)
                {
                    sum ++;
                }
            }
        }
        if(a>1)
        {
            sum++;
        }
        if (sum == 3&&check==0)
        {
            cout << "1" << endl;
        }
        else
        {
            cout << "0" << endl;
        }
    }
}CPP0118 - Sá» SPHENIC
