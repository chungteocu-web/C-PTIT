#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
using namespace std;
int binary(vector<int> &a, int x)
{
    int l = 0, r = a.size() - 1;
    while (l <= r)
    {
        int mid = (l + r) / 2;
        if (a[mid] == x)
            return 1;
        if (a[mid] < x)
        {
            l = mid + 1;
        }
        if (a[mid] > x)
        {
            r = mid - 1;
        }
    }
    return 0;
}
int main()
{
    int m, n, x;
    cin >> m;
    while (m--)
    {
        cin >> n;
        vector<int> a(n), b(n);
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
            b[i] = a[i];
        }
        sort(a.begin(), a.end());
        int l = 0, r = 0;
        for (int i = 0; i < n; i++)
        {
            if (a[i] != b[i])
            {
                l = i;
                break;
            }
        }
        for (int i = n - 1; i >= 0; i--)
        {
            if (a[i] != b[i])
            {
                r = i;
                break;
            }
        }

        cout << l + 1 << " " << r + 1 << endl;
    }
}
