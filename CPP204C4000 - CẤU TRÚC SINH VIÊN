#include <iostream>
#include <cmath>
#include <algorithm>
#include <string>
#include <iomanip>
using namespace std;
struct SinhVien
{
    string ten, masv, ngaysinh;
    double diem;
};
void inThongTinSV(struct SinhVien &a)
{
    if (a.ngaysinh[1] == '/')
    {
        a.ngaysinh.insert(0, "0");
    }
    if (a.ngaysinh[4] == '/')
    {
        a.ngaysinh.insert(3, "0");
    }
    cout << "N20DCCN001" << " " << a.ten << " " << a.masv << " " << a.ngaysinh << " " << fixed << setprecision(2) << a.diem;
}
void nhapThongTinSV(struct SinhVien &a)
{
    getline(cin, a.ten);
    cin >> a.masv >> a.ngaysinh >> a.diem;
}
int main(){
    struct SinhVien a;
    nhapThongTinSV(a);
    inThongTinSV(a);
    return 0;
}
