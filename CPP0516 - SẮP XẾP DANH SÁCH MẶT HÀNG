#include <iostream>
#include <iomanip>
#include <algorithm>
#include <vector>
#include <string>
#include <sstream>
using namespace std;
struct Mathang
{
    int ma;
    string ten, loai;
    double nhap, ban, loinhuan;
};
typedef struct Mathang Mathang;
bool sosanh(Mathang a, Mathang b)
{
    return a.loinhuan > b.loinhuan;
}
void xuat(Mathang ds[], int m)  
{
    sort(ds, ds + m, sosanh);
    for (int i = 0; i < m; i++)
    {
        cout << ds[i].ma << " " << ds[i].ten << " " << ds[i].loai << " " << fixed << setprecision(2) << ds[i].loinhuan << endl;
    }
}
void nhap(Mathang ds[], int m)
{
    for (int i = 0; i < m; i++)
    {
        ds[i].ma = i + 1;
        getline(cin, ds[i].ten);
        getline(cin, ds[i].loai);
        cin >> ds[i].nhap >> ds[i].ban;
        ds[i].loinhuan = ds[i].ban - ds[i].nhap;
        cin.ignore(1);
    }
}
int main()
{
    int m;
    cin >> m;
    cin.ignore(1);
    Mathang ds[m];
    nhap(ds, m);
    xuat(ds, m);
}
