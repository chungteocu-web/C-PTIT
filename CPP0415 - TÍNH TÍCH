#include <iostream>
#include <string>
#include <sstream>
#include <vector>
#include <algorithm>
using namespace std;
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        int a, b;
        cin >> a >> b;
        vector<long long> A(a), B(b);
        for (int i = 0; i < a; i++)
        {
            cin >> A[i];
        }
        for (int i = 0; i < b; i++)
        {
            cin >> B[i];
        }
        sort(A.begin(), A.end());
        sort(B.begin(), B.end());
        cout << B[0] * A[A.size() - 1] << endl;
    }
}
