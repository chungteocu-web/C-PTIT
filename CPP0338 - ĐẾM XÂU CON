#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
using namespace std;
int main()
{
    int m, n;
    cin >> m;
    while (m--)
    {
        int xaucon = 0;
        string s;
        cin >> s >> n;
        for (int i = 0; i < s.size(); i++)
        {
            int res = 0;
            map<char, int> mp;
            for (int j = i; j < s.size(); j++)
            {
                if (mp[s[j]] == 0)
                {
                    mp[s[j]]++;
                    ++res;
                }
                if (res == n)
                {
                    ++xaucon;
                }
                else if (res > n)
                {
                    break;
                }
            }
        }
        cout << xaucon << endl;
    }
}
