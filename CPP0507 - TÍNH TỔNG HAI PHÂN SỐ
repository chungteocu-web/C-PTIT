#include <iostream>
#include <string>
#include <cmath>
#include <algorithm>
#include <iomanip>
using namespace std;
struct PhanSo
{
    long long tu, mau;
};

long long ucln(long long a, long long b)
{
    a = abs(a);
    b = abs(b);
    while (b != 0)
    {
        long long x = a % b;
        a = b;
        b = x;
    }
    return a;
}
struct PhanSo tong(struct PhanSo &p, struct PhanSo &q)
{
    struct PhanSo x;
    long long t = p.mau * q.mau;
    long long tongtu = p.tu * q.mau + q.tu * p.mau;
    long long c = ucln(tongtu, t);
    x.tu = tongtu / c;
    x.mau = t / c;

    if (x.mau < 0)
    {
        x.tu = -x.tu;
        x.mau = -x.mau;
    }

    return x;
}
void in(struct PhanSo &t)
{
    cout << t.tu << "/" << t.mau;
}
void nhap(struct PhanSo &p)
{
    cin >> p.tu >> p.mau;
}
int main() {
	struct PhanSo p,q;
	nhap(p); nhap(q);
	PhanSo t = tong(p,q);
	in(t);
	return 0;
}
