#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
void xuly(vector<vector<int>> &matran, int a)
{
    for (int i = 0; i < a; i++)
    {
        for (int j = 0; j < a; j++)
        {
            if (i == 0 || i == a - 1 || j == 0 || j == a - 1)
                cout << matran[i][j] << " ";
            else
                cout << " " << " ";
        }
        cout << "\n";
    }
}
void nhap(vector<vector<int>> &matran, int a)
{
    for (int i = 0; i < a; i++)
    {
        for (int j = 0; j < a; j++)
        {
            cin >> matran[i][j];
        }
    }
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a;
        cin >> a;
        vector<vector<int>> matran(a, vector<int>(a));
        nhap(matran, a);
        xuly(matran, a);
    }
}
