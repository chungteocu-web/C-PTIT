#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
bool prime(int n)
{
    if (n == 2 || n == 3)
        return true;
    if (n < 3 || n % 2 == 0 || n % 3 == 0)
        return false;
    for (int i = 5; i * i <= n; i += 6)
        if (n % i == 0 || n % (i + 2) == 0)
            return false;
    return true;
}
int sotang(int m)
{
    int sodau = m % 10;
    m /= 10;
    while (m != 0)
    {
        int tmp = m % 10;
        if (sodau <= tmp)
        {
            return 0;
        }
        sodau = tmp;
        m /= 10;
    }
    return 1;
}
int sogiam(int m)
{
    int sodau = m % 10;
    m /= 10;
    while (m != 0)
    {
        int tmp = m % 10;
        if (sodau >= tmp)
        {
            return 0;
        }
        sodau = tmp;
        m /= 10;
    }
    return 1;
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int a, sum = 0;
        cin >> a;
        for (int i = pow(10, a - 1); i < pow(10, a); i++)
        {
            if (sotang(i) == 1 || sogiam(i) == 1)
            {
                if (prime(i))
                    ++sum;
            }
        }
        cout << sum << endl;
    }
}
