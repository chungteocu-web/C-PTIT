#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
using namespace std;
void sinh(int a[], int n, int k)
{
    int check = 0;
    for (int j = 0; j < k; j++)
    {
        a[j] = j + 1;
    }
    while (check == 0)
    {
        int i = k - 1;
        for (int j = 0; j < k; j++)
        {
            cout << a[j];
        }
        cout << " ";
        while (i >= 0 && a[i] == n - k + i+ 1)
        {
            i--;
        }
        if (i < 0)
            check = 1;
        else
        {
            a[i]++;
            for (int j = i + 1; j < k; j++)
            {
                a[j] = a[j - 1] + 1;
            }
        }
    }
}
int main()
{
    int m;
    cin >> m;
    while (m--)
    {
        int n, k;
        cin >> n >> k;
        int a[k];
        sinh(a, n, k);
        cout << endl;
    }
}
