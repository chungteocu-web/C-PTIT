#include <iostream>
#include <string>
#include <cmath>
#include <iomanip>
using namespace std;
long long ucln(long long a, long long b)
{
    while (b != 0)
    {
        int tmp = a % b;
        a = b;
        b = tmp;
    }
    return a;
}

int main()
{
    int c;
    cin >> c;
    while (c--)
    {
        long long a, bc = 1, uc;
        cin >> a;
        for (int i = 1; i <= a; i++)
        {
            long long s1 = i, s2 = bc;
            uc = ucln(s1, s2);
            bc = s1 * s2 / uc;
        }
        cout << bc << endl;
    }
}
