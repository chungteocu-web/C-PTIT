#include <iostream>
#include <algorithm>
#include <vector>
#include <cmath>
using namespace std;
int main()
{
    int n, k, x, m;
    cin >> m;
    while (m--)
    {
        cin >> n;
        vector<long long> a(n);
        for (int i = 0; i < n; i++)
        {
            long long tmp;
            cin >> tmp;
            a[i] = tmp * tmp;
        }
        int check = 0;
        sort(a.begin(), a.end());
        for (int i = 0; i < n; i++)
        {
            long long tong = 0;
            for (int j = 0; j < n; j++)
            {
                tong = a[i] + a[j];
                if (binary_search(a.begin(), a.end(), tong))
                {
                    check = 1;
                    break;
                }
            }
            if (check == 1)
            {
                break;
            }
        }
        if (check == 1)
        {
            cout << "YES\n";
        }
        else
        {
            cout << "NO\n";
        }
    }
}
