#include <iostream>
#include <algorithm>
#include <set>
#include <string>
#include <map>
using namespace std;
;
int main()
{
    int m;
    cin >> m;
    cin.ignore();
    while (m--)
    {
        map<int, int> mp;
        string s1, s2;
        cin >> s1;
        int sum = 0;
        for (int i = 0; i < s1.size(); i++)
        {
            if (s1[i] >= '0' && s1[i] <= '9')
            {
                sum += (s1[i] - '0');
            }
            else
            {
                s2.push_back(s1[i]);
            }
        }
        string tmp = to_string(sum);           
        sort(s2.begin(), s2.end());
        s2 += tmp;
        cout << s2 << endl;
    }
}
